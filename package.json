{
  "name": "no-dead-code",
  "version": "0.5.0",
  "description": "Find dead code in your project",
  "author": {
    "name": "Veikko Suhonen"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Veikkosuhonen/no-dead-code"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsc -w",
    "link": "pnpm link --global",
    "lint": "tsc",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "publish": "rm -r dist && pnpm run ci && pnpm publish --access public"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^20.11.5",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6",
    "vitest": "1.2.1"
  },
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.7",
    "@babel/types": "^7.23.6",
    "commander": "^11.1.0"
  },
  "keywords": [
    "refactoring",
    "refactor",
    "dead code",
    "dead",
    "unused",
    "unused code",
    "find unused",
    "find dead",
    "unused imports",
    "unused exports",
    "unused files",
    "unused modules",
    "dead-code-elimination",
    "code elimination"
  ]
}